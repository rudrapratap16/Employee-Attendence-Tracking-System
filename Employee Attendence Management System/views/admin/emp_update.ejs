<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Employee Name</th>
        <th scope="col">Employee Id</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <tbody>
      <%let i = 1%>
      <%for(user of users){%>
        <tr>
          <form method="post" action="/attendence/admin/employee/<%=user.id%>/delete?_method=DELETE">
          <th scope="row"><%=i%></th>
          <%i = i + 1%>
          <td><%=user.name%></td>
          <td><%=user.id%></td>
          <td><%=user.id%></td>
          <td><button>Delete</button></td>
        </form>
        </tr>
      <%}%>
</body>
</html>